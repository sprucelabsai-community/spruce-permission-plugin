{
  "name": "@sprucelabs/spruce-permission-plugin",
  "version": "29.0.127",
  "repository": "https://github.com/sprucelabsai-community/spruce-permission-plugin.git",
  "skill": {
    "namespace": "spruce-permission-plugin",
    "upgradeIgnoreList": [
      "@sprucelabs/spruce-permission-plugin",
      "@sprucelabs/spruce-event-utils"
    ],
    "blockedCommands": {
      "sync.listeners": "There are no listeners in this module."
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "build"
  ],
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "module": "./build/esm/index.js",
  "sideEffects": false,
  "keywords": [
    "node",
    "components",
    "sprucebot",
    "sprucelabs"
  ],
  "scripts": {
    "build.ci": "yarn build.tsc && yarn build.resolve-paths && yarn lint",
    "build.dev": "yarn build.tsc --sourceMap ; yarn resolve-paths.lint",
    "build.copy-files": "mkdir -p build && rsync -avzq --exclude='*.ts' ./src/ ./build/",
    "build.dist": "tsc --project tsconfig.dist.json && yarn build.resolve-paths && mv build esm && yarn build.esm-postbuild && yarn build.tsc && yarn build.resolve-paths && mv esm build/ && yarn clean.dist",
    "build.esm-postbuild": "esm-postbuild --target esm --patterns '**/*.js'",
    "build.resolve-paths": "resolve-path-aliases --target build --patterns '**/*.js,**/*.d.ts'",
    "build.tsc": "yarn build.copy-files && tsc",
    "clean": "yarn clean.build",
    "clean.all": "yarn clean.dependencies && yarn clean.build",
    "clean.build": "rm -rf build/",
    "clean.dependencies": "rm -rf node_modules/ package-lock.json yarn.lock",
    "clean.dist": "rm -rf build/__tests__ build/esm/__tests__",
    "fix.lint": "eslint --fix --cache '**/*.ts'",
    "lint": "eslint --cache '**/*.ts'",
    "lint.tsc": "tsc -p . --noEmit",
    "post.watch.build": "yarn build.copy-files && yarn build.resolve-paths",
    "rebuild": "yarn clean.all && yarn && yarn build.dev",
    "release": "semantic-release",
    "resolve-paths.lint": "yarn build.resolve-paths ; yarn lint",
    "test": "jest",
    "update.dependencies": "yarn clean.dependencies && yarn",
    "watch.build.dev": "tsc-watch --sourceMap --onCompilationComplete 'yarn post.watch.build'",
    "watch.lint": "concurrently 'yarn lint' \"chokidar 'src/**/*' -c 'yarn lint.tsc'\"",
    "watch.rebuild": "yarn clean.all && yarn && yarn watch.build.dev",
    "watch.tsc": "tsc -w"
  },
  "dependencies": {
    "@sprucelabs/error": "^5.1.9",
    "@sprucelabs/heartwood-view-controllers": "^105.0.5",
    "@sprucelabs/mercury-client": "^40.1.66",
    "@sprucelabs/mercury-core-events": "^21.1.55",
    "@sprucelabs/mercury-types": "^45.0.48",
    "@sprucelabs/schema": "^28.6.46",
    "@sprucelabs/spruce-core-schemas": "^38.1.47",
    "@sprucelabs/spruce-event-plugin": "^59.0.74",
    "@sprucelabs/spruce-permission-utils": "^5.0.113",
    "@sprucelabs/spruce-skill-utils": "^29.1.12"
  },
  "devDependencies": {
    "@sprucelabs/esm-postbuild": "^5.0.24",
    "@sprucelabs/jest-json-reporter": "^7.0.57",
    "@sprucelabs/jest-sheets-reporter": "^3.0.24",
    "@sprucelabs/resolve-path-aliases": "^1.1.226",
    "@sprucelabs/semantic-release": "^4.0.8",
    "@sprucelabs/spruce-skill-booter": "^59.0.74",
    "@sprucelabs/spruce-test-fixtures": "^59.0.74",
    "@sprucelabs/test": "^7.7.452",
    "@sprucelabs/test-utils": "^3.4.72",
    "@types/node": "^20.3.1",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.42.0",
    "eslint-config-spruce": "^10.12.0",
    "jest": "^29.5.0",
    "jest-circus": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.1.3"
  },
  "description": "Register permissions on the boot of your skill",
  "jest": {
    "testRunner": "jest-circus/runner",
    "maxWorkers": 4,
    "testTimeout": 120000,
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/tmp/",
      "<rootDir>/src/",
      "<rootDir>/node_modules/",
      "<rootDir>/build/__tests__/testDirsAndFiles/"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js?(x)"
    ],
    "moduleNameMapper": {
      "^#spruce/(.*)$": "<rootDir>/build/.spruce/$1"
    },
    "reporters": [
      "default",
      [
        "@sprucelabs/jest-sheets-reporter",
        {
          "sheetId": "1DoWM7mYgGDa-PJXptLomrZmbIBZiuxS1YiAsRXU2Gm0",
          "worksheetId": 889384139,
          "testMap": {
            "registersPermissionsFromCombinedFile": "P12"
          }
        }
      ]
    ]
  }
}
